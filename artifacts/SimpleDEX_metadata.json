					"params": {
						"_token": "The address of the token whose price is being queried"
					},
					"returns": {
						"_0": "The price of the given token in terms of the other token (scaled to 1e18)"
					}
				},
				"removeLiquidity(uint256,uint256)": {
					"params": {
						"amountA": "The amount of Token A to be removed from the liquidity pool",
						"amountB": "The amount of Token B to be removed from the liquidity pool"
					}
				},
				"swapAforB(uint256)": {
					"params": {
						"amountAIn": "The amount of Token A to be exchanged for Token B"
					}
				},
				"swapBforA(uint256)": {
					"params": {
						"amountBIn": "The amount of Token B to be exchanged for Token A"
					}
				}
			},
